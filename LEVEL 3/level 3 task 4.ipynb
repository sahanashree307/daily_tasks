    "def fib(n):\n",
    "   \n",
    "    a, b = 0, 1\n",
    "    for _ in range(n):\n",
    "        yield a  \n",
    "        a, b = b, a + b  \n",
    "fibonacci_numbers = list(fib(5))\n",
    "print(fibonacci_numbers)"
   ]
