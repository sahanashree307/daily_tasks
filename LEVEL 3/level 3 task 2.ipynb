    "#task 3\n",
    "#basic class with inheritance\n",
    "class animal:\n",
    "    def sound(self):\n",
    "        return \"the sound of the animal\"\n",
    "\n",
    "class dog(animal):\n",
    "    def sound(self):\n",
    "        return \"woof\"\n",
    "\n",
    "dog_instance=dog()\n",
    "result=dog_instance.sound()\n",
    "print(result)"
   ]
  }
